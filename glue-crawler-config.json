{
  "Name": "chirps-crawler",
  "Role": "AfriclimateGlueRole",
  "DatabaseName": "africlimate_climate_db",
  "Targets": {
    "S3Targets": [
      {
        "Path": "s3://africlimate-analytics-lake/raw/chirps_monthly/",
        "Exclusions": []
      }
    ]
  },
  "Schedule": "cron(0 2 * * ? *)",
  "SchemaChangePolicy": {
    "UpdateBehavior": "LOG",
    "DeleteBehavior": "LOG"
  },
  "RecrawlPolicy": {
    "RecrawlBehavior": "CRAWL_NEW_FOLDERS_ONLY"
  }
}
